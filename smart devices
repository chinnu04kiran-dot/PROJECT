
public class home {

}
//Abstract class representing a generic smart device
abstract class SmartDevice {
 protected String deviceName;

 public SmartDevice(String name) {
     this.deviceName = name;
 }

 public abstract void connect();

 public void disconnect() {
     System.out.println(deviceName + " disconnected.");
 }

 // Called by Garbage Collector before object is removed (deprecated in Java 9+)
 @Override
 protected void finalize() throws Throwable {
     try {
         System.out.println("Cleaning up resources for " + deviceName);
         disconnect();
     } finally {
         super.finalize();
     }
 }
}
